<% include ../partials/header %>
<div class="container">
    <h1><%= detail.ProductName %></h1>
    <div class="form-group">
        <label for="">ID</label>
        <input type="text" name="" id="" class="form-control" placeholder="<%= detail.ProductID %>"
            aria-describedby="helpId" disabled>
        <small id="helpId" class="text-muted">Help text</small>
    </div>
    <div class="form-group">
            <label for="">Title</label>
            <input type="text" name="" id="" class="form-control" placeholder="<%= detail.ProductTitle %>"
                aria-describedby="helpId" disabled>
            <small id="helpId" class="text-muted">Help text</small>
        </div>
    <div class="form-group">
        <label for="">Price</label>
        <input type="text" name="" id="" class="form-control" placeholder="<%= detail.ProductPrice %>"
            aria-describedby="helpId" disabled>
        <small id="helpId" class="text-muted">Help text</small>
    </div>
    <div class="form-group">
        <label for="">Create At</label>
        <input type="text" name="" id="" class="form-control" placeholder="<%= detail.Create At %>"
            aria-describedby="helpId" disabled>
        <small id="helpId" class="text-muted">Help text</small>
    </div>
    <a href="/products"><button type="button" class="btn btn-primary">Save</button></a>
    <p class="lead">
        <a product="btn btn-primary btn-lg" href="Jumbo action link" role="button">Cancel</a>
    </p>
</div>
<% include ../partials/footer %>